<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.streetvendorhelpernew">

    <uses-permission android:name="android.permission.WAKE_LOCK" /> <!-- <uses-feature android:name="android.hardware.type.watch" /> -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_rounder_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.streetvendorhelpernew.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <uses-library -->
        <!-- android:name="com.google.android.wearable" -->
        <!-- android:required="true" /> -->
        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->
        <meta-data
                android:name="com.google.android.wearable.standalone"
                android:value="true" />

        <activity
                android:name="com.streetvendorhelpernew.ui.login.LoginActivity"
                android:label="@string/title_activity_login"
                android:parentActivityName="com.streetvendorhelpernew.MainActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.streetvendorhelpernew.MainActivity" />
        </activity>
        <activity android:name="com.streetvendorhelpernew.MainActivity">

        </activity>
        <activity
                android:name="com.streetvendorhelpernew.activity.LocationActivity"
                android:screenOrientation="portrait"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
                android:name="com.streetvendorhelpernew.activity.EventDetailsActivity"
                android:screenOrientation="portrait"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
                android:name="com.streetvendorhelpernew.activity.EventEditorActivity"
                android:screenOrientation="portrait"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
                android:name="com.streetvendorhelpernew.activity.FeedbackActivity"
                android:screenOrientation="portrait" />

        <meta-data
                android:name="com.google.android.geo.API_KEY"
                android:value="AIzaSyAk8ukOneB8ftARlhfalQ6eXmWITkV4_xc" />
        <meta-data
                android:name="io.fabric.ApiKey"
                android:value="9fb2a6e8b0714d426d26882dae257c079728fddf" />

        <provider
                android:name="androidx.core.content.FileProvider"
                android:authorities="com.streetvendorhelpernew.fileprovider"
                android:exported="false"
                android:grantUriPermissions="true">
            <meta-data
                    android:name="android.support.FILE_PROVIDER_PATHS"
                    android:resource="@xml/file_paths" />
        </provider>

        <service
                android:name="com.streetvendorhelpernew.Notification.MyFirebaseMessaging"
                android:icon="@mipmap/ic_launcher"
                android:stopWithTask="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>